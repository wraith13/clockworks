{
    "applicationTitle": "Clockworks",
    "localDbPrefix": "clockworks",
    "repositoryUrl": "https://github.com/wraith13/clockworks/",
    "rainbowColorSetDefaultIndex": 2,
    "rainbowColorSetSolidIndexRate": 7,
    "rainbowColorSet":
    [
        "#7E3397", "#6F39A1", "#5F42A7", "#504EA9",
        "#445DA8", "#3A6CA2", "#347C99", "#318A8C",
        "#33977E", "#39A16F", "#42A75F", "#4EA950",
        "#5DA844", "#6CA23A", "#7C9934", "#8A8C31",
        "#977E33", "#A16F39", "#A75F42", "#A9504E",
        "#A8445D", "#A23A6C", "#99347C", "#8C318A"
    ],
    "flashIntervalPreset": [ "0m", "1m", "2m", "3m", "4m", "5m", "6m", "10m", "12m", "15m", "20m", "30m", "60m" ],
    "timerPreset": [ "1m", "2m", "3m", "4m", "5m", "6m", "10m", "12m", "15m", "20m", "30m", "60m" ],
    "recentlyFlashIntervalMaxHistory": 3,
    "recentlyTimerMaxHistory": 8,
    "theme":
    {
        "original":
        {
            "@main-color": "#000022",
            "@base-color": "#FFFFFF",
            "@accent-color": "#5F42A7",
            "::-webkit-calendar-picker-indicator": "::-webkit-calendar-picker-indicator"
        },
        "light":
        {
            "@main-color": "#000022",
            "@base-color": "#FFFFFF",
            "@accent-color": "#5F42A7",
            "::-webkit-calendar-picker-indicator": ".dummy-invalid-class"
        },
        "dark":
        {
            "@main-color": "#FFFFFF",
            "@base-color": "#000022",
            "@accent-color": "#5F42A7",
            "::-webkit-calendar-picker-indicator": "::-webkit-calendar-picker-indicator"
        }
    },
    "timezoneOffsetList":
    [
        -840, -780, -765, -720, -660, -630, -600, -570, -540, -525, -480, -420, -390, -360, -345, -330, -300, -270, -240, -210, -180, -120, -60, 0, 60, 120, 180, 210, 240, 300, 360, 420, 480, 540, 570, 600, 660, 720
    ],
    "initialTimezoneList":
    [
        {
            "title": "timezone.Tokyo",
            "offset": -540
        },
        {
            "title": "timezone.Delhi",
            "offset": -330
        },
        {
            "title": "timezone.Istanbul",
            "offset": -180
        },
        {
            "title": "timezone.London",
            "offset": 0
        },
        {
            "title": "timezone.New York",
            "offset": 300
        },
        {
            "title": "timezone.Seattle",
            "offset": 480
        }
    ]
}
