{
  "name": "never-stop-watch",
  "version": "1.0.0",
  "description": "Never Stop Watch Static Web Application",
  "scripts": {
    "build all": "node ./build.js release",
    "build html": "node ./build.js none",
    "build style": "lessc ./style/index.less ./style/index.css",
    "build script": "tsc -p ./script/tsconfig.json",
    "debug build all": "node ./build.js debug",
    "debug build style": "lessc --source-map-map-inline ./style/index.less ./style/index.css",
    "debug build script": "tsc --inlineSourceMap -p ./script/tsconfig.json",
    "watch script": "tsc --watch --inlineSourceMap -p ./script/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wraith13/cnever-stop-watch.git"
  },
  "author": "wraith13",
  "license": "BSL-1.0",
  "bugs": {
    "url": "https://github.com/wraith13/never-stop-watch/issues"
  },
  "homepage": "https://github.com/wraith13/never-stop-watch",
  "devDependencies": {
    "less": "^4.1.1",
    "typescript": "^4.2.3"
  }
}
